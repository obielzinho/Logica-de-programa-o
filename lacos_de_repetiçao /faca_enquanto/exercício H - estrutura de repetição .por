programa {
  funcao inicio() {

    inteiro numero, maior=0, menor=0 

	    faca{
	    	escreva("Escreva um numero: ")
	    	leia(numero)
         se(numero > maior){
         	maior= numero 

         }
	    	se(numero < menor){
	    		menor= numero
	    	}
        
	    } enquanto( numero >=0)
	    
	     escreva("\nO menor valor digitado é: ", menor)
	     escreva("\nO maior valor digitado é: ", maior)
    
  }
}
